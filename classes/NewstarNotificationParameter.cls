public class NewstarNotificationParameter {
    
    public String Name {get; set;}

    public String Value {get; set;}

    public Boolean IsAutoGenerated {get; set;}
    
    
    public NewstarNotificationParameter() {

        this.Value='';
        this.IsAutoGenerated=false;
    }

    public NewstarNotificationParameter(String name, String value) {
        this.Name = name;
        this.Value = value;
        this.IsAutoGenerated=false;
    }
}